<h4 style="padding:0px;margin:0px;color:#666;font-size:17px">Personal Details</h4> 	
<table width="100%" align="center" style="border:1px solid #ccc" cellspacing="0" cellpadding="0">

<tbody><tr style="background:#e8e8e8;color:#666">
<th style="border:solid 1px #dddddd" height="30">Customer Name</th>
<th style="border:solid 1px #dddddd">Phone Number</th>
<th style="border:solid 1px #dddddd">Email</th>
</tr>
<tr style="color:#666;background:#fcfcfc">
<td align="center" style="border:solid 1px #dddddd" height="30">  {{ customer.name }} </td>
<td align="center" style="border:solid 1px #dddddd"> {{ customer.mobile }} </td>
<td align="center" style="border:solid 1px #dddddd"><a href="mailto:{{ customer.email }}" target="_blank"> {{ customer.email }} </a></td>
</tr>
</tbody></table>
<br>
<h4 style="padding:0px;margin:0px;color:#666;font-size:17px">Booking Details</h4>
<table width="100%" align="center" style="border:1px solid #ccc" cellpadding="0" cellspacing="0">
<tbody><tr style="background:#e8e8e8;color:#666">
<th style="border:solid 1px #dddddd" height="30">Booking Id #</th>
<th style="border:solid 1px #dddddd">Booked On </th>
</tr>
<tr style="color:#666;background:#fcfcfc">
<td align="center" style="border:solid 1px #dddddd" height="30">{{ booking.bookingId }}</td>
<td align="center" style="border:solid 1px #dddddd">{{ booking.bookedOn|date('d/m/Y') }}</td>
</tr>
</tbody></table>
<br>
<h4 style="padding:0px;margin:0px;color:#666;font-size:17px">Fare Details</h4>
<table width="100%" align="center" style="border:1px solid #ccc" cellpadding="0" cellspacing="0">
<tbody><tr style="background:#e8e8e8;color:#666">
<th style="border:solid 1px #dddddd" height="30">Total Fare</th>
<th style="border:solid 1px #dddddd">Discount</th>
<th style="border:solid 1px #dddddd">SGST</th>
<th style="border:solid 1px #dddddd">CGST</th>
<th style="border:solid 1px #dddddd">Amount Paid</th>
<th style="border:solid 1px #dddddd">Balance Amount</th>
</tr>
<tr style="color:#666;background:#fcfcfc">
<td align="center" style="border:solid 1px #dddddd" height="30">Rs {{ booking.totalPrice }} </td>
<td align="center" style="border:solid 1px #dddddd">Rs {{ booking.discount }}</td>
<td align="center" style="border:solid 1px #dddddd">Rs {{ booking.swachBharthCess }} </td>
<td align="center" style="border:solid 1px #dddddd">Rs {{ booking.krishiKalyanCess }} </td>
<td align="center" style="border:solid 1px #dddddd"> Rs {{ booking.amountPaid }} </td>
<td align="center" style="border:solid 1px #dddddd">Rs {{ booking.finalPrice - booking.amountPaid }} </td>
</tr>

</tbody></table>
<table width="100%" align="center" style="border:1px solid #ccc" cellpadding="0" cellspacing="0">
<tbody><tr style="background:#e8e8e8;color:#666">
<th style="border:solid 1px #dddddd" height="30">Bike Name</th>
<th style="border:solid 1px #dddddd">Start Date</th>
<th style="border:solid 1px #dddddd">End Date</th>
<th style="border:solid 1px #dddddd">Days</th>
<th style="border:solid 1px #dddddd">Hours</th>
<th style="border:solid 1px #dddddd">City</th>
<th style="border:solid 1px #dddddd">Area</th>
</tr>
<tr style="color:#666;background:#fcfcfc">
                            {% for bikesbooking in bikesbookings %}
                             	{% if booking.id==bikesbooking.bikebookingid %}
                             	<td align="center" style="border:solid 1px #dddddd">{{ bikesbooking.bikename }}</td>
                             	<td align="center" style="border:solid 1px #dddddd">{{ bikesbooking.pdate }}</td>
                             	<td align="center" style="border:solid 1px #dddddd">{{ bikesbooking.rdate }}</td>
                             	<td align="center" style="border:solid 1px #dddddd">{{ bikesbooking.leftdays }}</td>
                             	<td align="center" style="border:solid 1px #dddddd">{{ bikesbooking.hours }}</td>
                             	<td align="center" style="border:solid 1px #dddddd">{{ bikesbooking.bikelocation }}</td>
                             	<td align="center" style="border:solid 1px #dddddd">{{ bikesbooking.bikearea }}</td>
                             	{% endif %}
                        	 {% endfor %}
</tr>

</tbody></table>
