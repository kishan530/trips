{% extends '::base.html.twig' %}
{% trans_default_domain 'FOSUserBundle' %}
{% block body %}

	<!-- Container -->
	<div id="container">
		<!-- Start Page Banner -->
		<div class="page-banner backgroundImage">
			<div class="container">
				<div class="row">
					<div class="col-md-6">
						<h2>Register</h2>
						
					</div>
					<div class="col-md-6">
						<ul class="breadcrumbs">
							<li><a href="{{ path('drive_home_homepage') }}">Home</a></li>
							<li>Register</li>
						</ul>
					</div>
				</div>
			</div>
		</div>
		<!-- End Page Banner -->
		
{% block fos_user_content %}
<div id="content">
 <div class="container">
        <div class="row">
        	 <div class="col-md-10 center msg top-buffer2 bottom buffer2 ">
		<p><label>{{ 'registration.confirmed'|trans({'%username%': user.username}) }}</label></p>
		{% if app.session is not empty %}
			{% set targetUrl = app.session.get('_security.' ~ app.security.token.providerKey ~ '.target_path') %}
			{% if targetUrl is not empty %}<p><a href="{{ targetUrl }}">{{ 'registration.back'|trans }}</a></p>{% endif %}
		{% endif %}
			</div>
	</div>
	</div>
</div>
{% endblock fos_user_content %}
</div>
{% endblock %}