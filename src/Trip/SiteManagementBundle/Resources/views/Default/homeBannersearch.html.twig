<div class="bike-book homebanner-padding clearfix">
{% if bikemainloc %}
		<h2 class="sub-title-bikesonrent"> 
		Rent A Bike In 
		    {{ bikemainloc }}
		</h2>
{% else %}
		<h2 class="sub-title-bikesonrent"> Rent A Bike In Bengaluru</h2>
{% endif %}
		<input type="hidden" value="{{ bikemainloc }}" id="txt_name1">
        {#}<img src="{{asset('images/bikes/s2.jpg') }}" style="width: 100%;">{#}
        <div class="container bike-price-book1">
        	<div class="row">
       			<div class="col-md-2"></div>
       				<div class="col-md-8 sub-book1">
       					<div class="row">
       					<div class="col-md-6">
       						<table class="table" style="display:none">
                   					<tbody>
                   						<tr>
                   							<td class="text-center" style="background: transparent;border:none"></td>
                   							<td class="bikes-search-tabs" style="background: #F34F4F;border: none;">Bikes</td>
                   							<td class="text-center" style="background: transparent;border:none"></td>
                   							<td class="text-center" style="background: transparent;border:none"><a href="#tab_04" aria-controls="tab_04" role="tab" data-toggle="tab">Offers</a></td>
                   							
                   						</tr>
                   					</tbody>
                   				</table>
       					</div>
       					<div class="col-md-6">
       						
       					</div>
                           	
                         </div>
       				</div>
       				<div class="col-md-2"></div>   
       			</div>
       		<div class="row">
       			<div class="col-md-2"></div>
       				<div class="col-md-8">
       					<div class="row">
                             {{ form_start(form) }}
                             {{ form_errors(form) }}
                             {{ form_errors(form.date) }}
                             {{ form_errors(form.returndate) }}
								<div class="col-md-3 bikes-form-font sub-book3">
										<div class="dropdown shaow">
                                                        <div class="select-city-height dropdown-toggle" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                            {% if filtermainloc %}
                                                             <span class="select-city caps">{{ filtermainloc }} <i class="fa fa-angle-down"></i></span>
                                                            {% else %}
                                                             <span class="select-city caps">Select City <i class="fa fa-angle-down"></i></span>
                                                            {% endif %}
                                                        </div> 
                                                        <div class="dropdown-menu select-city-links" aria-labelledby="dropdownMenuLink">
                                                             <a  href="{{ path('trip_site_management_bikes_city_sybmit') }}?city=1&name=tirupathi" class="caps bikes-search-popup-heading2 text-center resview-bikes-pop-loc">tirupathi</a>	<br>
                                                             <a  href="{{ path('trip_site_management_bikes_city_sybmit') }}?city=2&name=bengaluru" class="caps bikes-search-popup-heading2 text-center resview-bikes-pop-loc">bengaluru</a>	<br>
                                                         </div>
                                                    </div>			
                                </div>
                             	<div class="col-md-3 bikes-form-font sub-book3">
                                {{ form_widget(form.date, {'id': 'datetime2'}) }}
                                </div>
                                <div class="col-md-3 bikes-form-font sub-book3">
                                	{{ form_widget(form.returndate, {'id': 'datetime3'}) }}
                                </div>
                                <div class="col-md-3 sub-book4">
                                {{ form_label(form.submit) }} 
                                {{ form_widget(form.submit) }}
                                </div>
                             {{ form_end(form) }}
                         </div>
       				</div>
       				<div class="col-md-2"></div>   
       			</div>
       			{#}<h3 class="home-banner-offer">Just go</h3>{#}
       			<img src="{{ asset('images/Just-Go-web-dark-bg.png') }}" alt="Trips" style="width: 180px;">
    			<p class="home-banner-offer-p">Rent A Bike @ Rs.3/Km*</p>
        </div>  
    </div>
    <div style="padding: 20px 0px;">
    
    </div>
