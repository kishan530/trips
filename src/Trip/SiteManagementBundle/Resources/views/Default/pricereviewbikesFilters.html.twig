<h3 class="review-apply-filter upper">Search By Locations</h3>
<div class="row">
    	<div class="col-md-10">
    		<input type="text" id="filterloc" placeholder="Search Location" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Search Location'" class="search-location-infilters" >
    	</div>
    	<div class="col-md-2" style="float:right">
    		<i class="fa fa-search" style="color:#222;"></i>
    	</div>
</div>
<div class="row"><div class="col-md-12"><hr class="hr-nomargin" style="margin-top: -10px;"></div></div>
<div class="review-filters-loc" id="searchloc">
	<table id="" class="myListloc" style="background-color:#F3F3F3;" border="0">
        <tbody>
        {% for key,areas in bikesarea %}
            <tr>
                <td style="background: #fff;">
                    <input type="checkbox" name="price_check_list_loc[]" value="{{ areas.area }}" 
                    onclick="this.form.submit()" {% for checklist in filterloc %}{% if checklist== areas.area %} checked {% endif %}{% endfor %} > 
                    <label for="">{{ areas.area }}</label>
                </td>      
            </tr>
         {% endfor %}
            </tbody>
    </table>
</div>

<h3 class="review-apply-filter upper">Search By Bikes</h3>
<div class="row">
	<div class="col-md-10">
		<input type="text" id="filterbikes" placeholder="Search Bikes" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Search Bikes'" class="search-location-infilters" >
	</div>
	<div class="col-md-2" style="float:right">
		<i class="fa fa-search" style="color:#222;"></i>
	</div>
</div>
<div class="row"><div class="col-md-12"><hr class="hr-nomargin" style="margin-top: -10px;"></div></div>
<div class="review-filters-bikes">
	<table id="" class="myListbikes" style="background-color:#F3F3F3;" border="0">
        <tbody>
            {% for bike in bikes %}
            	<tr>
                <td style="background: #fff;">
                    <input type="checkbox" name="price_check_list_bikes[]" value="{{ bike.id }}" onclick="this.form.submit()"
                    {% for checklist in filterbikes %}{% if checklist== bike.id %} checked {% endif %}{% endfor %} {% if url== bike.id %} checked {% endif %}> 
                    <label for="">{{ bike.title }}</label><br>
                 </td>
               </tr>
            {% endfor %}
    	</tbody>
    </table>
</div>