	<script>
	$( window ).load(function getlocation(){
		if(navigator.geolocation){
			navigator.geolocation.getCurrentPosition(showPosition);
			function showPosition(position){
				//x.innerHTML = "latitude=" + position.coords.latitude;
				//x.innerHTML += "<br />"
				//x.innerHTML += "latitude=" + position.coords.longitude
				
				lat = position.coords.latitude;
				lang = position.coords.longitude;
				
				$('#lat').val(lat);
				$('#lang').val(lang);
				var lat = parseFloat(document.getElementById("lat").value);
	            var lng = parseFloat(document.getElementById("lang").value);
	            var latlng = new google.maps.LatLng(lat, lng);
	            var geocoder = geocoder = new google.maps.Geocoder();
	            geocoder.geocode({ 'latLng': latlng }, function (results, status) {
	                if (status == google.maps.GeocoderStatus.OK) {
	                    if (results[1]) {
	                        //alert("Location: " + results[1].formatted_address);
	    					address = results[11].formatted_address;
	                        $('#address').val(address);
	                        if(address == 'Tirupati, Andhra Pradesh, India'){
	                        	//document.forms['locationtraktpt'].submit();
	                        	location.href = document.getElementById("mylocationtpt");
	                        	$('#addresstpt').val(address);
		                        }
	                        else{
	                        	//document.forms['locationtrakblr'].submit();
	                        	location.href = document.getElementById("mylocationblr");
	                        	$('#addressblr').val(address);
		                        }
	                    }
	                }
	            });
			}
		}else{
			x.innerHTML = "Browser not Supporting";
		}

		
	});
	
	var x =document.getElementById('output');
	</script>
