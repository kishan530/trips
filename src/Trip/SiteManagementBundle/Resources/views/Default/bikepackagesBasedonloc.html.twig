			var from = $("#datetime6_prefer_date").val().split("-");
			var year = from[2].split(" ");
			var from2 = year[0]+'.'+from[1]+'.'+from[0];
			var t = new Date(from2);
			//alert(t.getDate());
			t.setDate(t.getDate() + 7);
			$('#datetime7_returnDate').datetimepicker({
				minDate: t,
			    });
			
			
			//$('#datetime7_returnDate').handleDtpicker('setMinDate', t);
